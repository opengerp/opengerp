<schema>
    <table name="INT_Moduli" config="true" dbobject="Moduli" group="opengerp:users">
        <field name="ID" type="varchar(20)" null="NO"  primary_key="yes"/>
        <field name="Descrizione" type="varchar(255)" null="NO" default=""/>
        <field name="Descrizione_Ext" type="text" null="YES" default=""/>
        <field name="Path" type="varchar(255)" null="YES" default="" comment="obsoleto"/>
        <field name="Controller" type="varchar(255)" null="NO" default=""/>
        <field name="Voce_Menu" type="varchar(255)" null="NO" default=""/>
        <field name="Get_Vars" type="varchar(255)" null="YES" default=""/>
        <field name="ID_Parent" type="varchar(50)" null="NO" default="0"/>
        <field name="Custom" type="smallint(4)" null="NO" default="0"/>
        <field name="Json_Config" type="text" null="YES" default=""/>
        <field name="Json_Parametri_Utenti" type="text" null="YES" default=""/>

        <field name="Widget" type="varchar(255)" null="YES" default=""/>

        <index name="PRIMARY">
            <field name="ID" seq="1"/>
        </index>

    </table>

    <table name="INT_Moduli_Lang" config="true">
        <field name="ID_Modulo" type="varchar(20)" null="NO"  primary_key="yes"/>
        <field name="Cod_Lingua" type="char(2)" null="NO"  primary_key="yes"/>
        <field name="Descrizione" type="varchar(255)" null="NO" default=""/>
        <field name="Voce_Menu" type="varchar(255)" null="NO" default=""/>

        <index name="PRIMARY">
            <field name="ID_Modulo" seq="1"/>
            <field name="Cod_Lingua" seq="2"/>

        </index>

    </table>


    <table name="INT_Menu" config="true" dbobject="MenuItems" group="opengerp:users">
        <field name="ID_Menu" type="varchar(255)" null="NO"  primary_key="yes"/>
        <field name="Voce_Menu" type="varchar(255)" null="NO" default=""/>
        <field name="Icon" type="varchar(255)" null="NO" default=""/>

        <field name="Json_Traduzione" type="text" null="NO" default=""/>

        <index name="PRIMARY">
            <field name="ID_Menu" seq="1"/>
        </index>

    </table>

    <table name="INT_Utenti" config="true" entity="Users" dbobject="Users" group="opengerp:users">
        <field name="ID" type="int(11)" null="NO"  primary_key="yes" extra="auto_increment"/>
        <field name="Nome" type="varchar(50)" null="YES" default=""/>
        <field name="Cognome" type="varchar(50)" null="YES" default=""/>
        <field name="Titolo" type="varchar(50)" null="YES" default=""/>
        <field name="Des_Azienda" type="varchar(255)" null="YES" default=""/>
        <field name="Azienda" type="char(1)" null="YES" default="NULL" comment="obsoleto"/>
        <field name="ID_Cliente_Utente" type="int(11)" null="YES" default="NULL"/>

        <field name="Data_Nascita" type="varchar(50)" null="YES" default=""/>
        <field name="Cod_Localita_Nas" type="varchar(20)" null="YES" default=""/>
        <field name="Cod_Nazione_Nas" type="char(3)" null="YES" default=""/>
        <field name="Des_Localita_Nas" type="varchar(255)" null="YES" default=""/>

        <field name="Cod_Cap" type="varchar(10)" null="YES" default=""/>
        <field name="Sig_Prov" type="char(2)" null="YES" default=""/>

        <field name="Indirizzo" type="varchar(255)" null="YES" default=""/>
        <field name="Indirizzo2" type="varchar(255)" null="YES" default=""/>

        <field name="Des_Localita" type="varchar(255)" null="YES" default=""/>
        <field name="Codice_Fiscale" type="varchar(16)" null="YES" default=""/>
        <field name="Cod_Localita" type="varchar(50)" null="YES" default=""/>
        <field name="Cod_Nazione" type="varchar(3)" null="YES" default=""/>
        <field name="Fisso" type="varchar(50)" null="YES" default=""/>
        <field name="Mobile" type="varchar(50)" null="YES" default=""/>
        <field name="Interno" type="varchar(50)" null="YES" default=""/>
        <field name="Email" type="varchar(127)" null="YES" default=""/>
        <field name="Email_Intranet" type="varchar(127)" null="YES" default=""/>
        <field name="Note_Utente" type="varchar(255)" null="YES" default=""/>
        <field name="ID_Role" type="int(11)" null="NO" default="0"/>
        <field name="ID_Reparto" type="int(11)" null="YES" default="NULL"/>
        <field name="Stato_Utente" type="smallint(6)" null="YES" default="NULL"/>
        <field name="Utente" type="int(11)" null="YES" default="NULL"/>
        <field name="Str_Activation" type="varchar(255)" null="YES" default=""/>
        <field name="Date_Inserted" type="datetime" null="YES" default="NULL"/>
        <field name="Data_Ultimo_Agg" type="datetime" null="YES" default="NULL"/>

        <field name="Json_Utente" type="text" null="YES" default=""/>

        <index name="PRIMARY">
            <field name="ID" seq="1"/>
        </index>

        <index name="ID_Cliente_Utente">
            <field name="ID_Cliente_Utente" seq="1"/>
        </index>
        <index name="Email">
            <field name="Email" seq="1"/>
        </index>

    </table>


    <table name="INT_Utenti_Aut" config="true" dbobject="UserAuth" group="opengerp:users">
        <field name="Username" type="varchar(50)" null="NO" default="" primary_key="yes"/>
        <field name="ID_Utente" type="varchar(50)" null="YES" default=""/>
        <field name="Password" type="varchar(60)" null="YES" default=""/>
        <field name="Lastlogin" type="datetime" null="YES" default=""/>
        <field name="IP" type="varchar(15)" null="YES" default=""/>
        <field name="Ab_Internet" type="smallint(6)" null="NO" default="0"/>
        <field name="Ab_Vis_Costi" type="smallint(6)" null="NO" default="0"/>
        <field name="Ultimo_IP" type="varchar(15)" null="YES" default=""/>
        <field name="Tipo_Menu" type="smallint(6)" null="NO" default="1"/>
        <field name="Data_Cambio_Password" type="datetime" null="YES" default="NULL"/>
        <field name="Cod_Lingua" type="varchar(15)" null="NO" default="IT"/>
        <field name="Parametri_Email" type="text" null="YES" default=""/>
        <field name="Parametri_Aut" type="text" null="YES" default=""/>
        <field name="Token_Api" type="varchar(255)" null="YES" default=""/>
        <field name="Authenticator_Secret" type="varchar(20)" null="YES" default="NULL"/>

        <index name="PRIMARY">
            <field name="Username" seq="1"/>
        </index>

        <index name="ID_Utente">
            <field name="ID_Utente" seq="1"/>
        </index>
    </table>


    <table name="INT_Utenti_Moduli" config="true" group="opengerp:users">
        <field name="ID_Utente" type="int(11)" null="NO" default="0" primary_key="yes"/>
        <field name="ID_Modulo" type="varchar(50)" null="NO" default="" primary_key="yes"/>
        <field name="Parametri" type="varchar(255)" null="YES" default=""/>
        <field name="Json_User_Params" type="text" null="YES" default=""/>

        <index name="PRIMARY">
            <field name="ID_Utente" seq="1"/>
            <field name="ID_Modulo" seq="2"/>
        </index>

    </table>


    <table name="MAG_Causali" config="true" group="opengerp/accounting">
        <field name="ID_Causale" type="int(11)" null="NO" default="0" primary_key="yes"/>
        <field name="Des_Causale" type="varchar(255)" null="YES" default=""/>
        <field name="Tipo_Controparte" type="smallint(6)" null="YES" default=""/>
        <field name="Op_Q_Esistente" type="char(1)" null="YES" default=""/>
        <field name="Op_Q_Impegnata" type="char(1)" null="YES" default=""/>
        <field name="Op_Q_Ordinata" type="char(1)" null="YES" default=""/>
        <field name="Ric_Impegno" type="smallint(6)" null="YES" default=""/>

        <index name="PRIMARY">
            <field name="ID_Causale" seq="1"/>
        </index>

    </table>



    <table name="COG_Modelli" config="true" group="opengerp/accounting">
        <field name="Cod_Modello" type="varchar(100)" null="NO" primary_key="yes"/>
        <field name="Des_Modello" type="varchar(255)" null="NO" default=""/>
        <field name="Serie_Modello" type="varchar(10)" null="YES" default="" comment="da usare in caso di serie diverse es. per diversi modelli reverse charge"/>
        <field name="Operazione" type="int(11)" null="NO" />
        <field name="Op_Partitario" type="int(11)" null="NO" />
        <field name="Ric_Documento" type="int(11)" null="NO" />
        <field name="Tipo" type="varchar(255)" null="NO" default=""/>
        <field name="Reg_Iva" type="varchar(10)" null="YES" default=""/>
        <field name="Predefinito" type="smallint(6)" null="NO" default="0"/>
        <field name="Disabilitato" type="smallint(6)" null="NO" default="0"/>
        <field name="SegnoIva" type="smallint(6)" null="YES" default=""/>
        <field name="Cod_Tipo_Anagrafica" type="char(1)" null="YES" default=""/>
        <field name="Note_Modello" type="varchar(255)" null="YES" default=""/>
        <field name="Ab_Num_Autonoma" type="smallint(6)" null="NO" default="0"/>
        <field name="Cod_Modello_Collegato" type="varchar(50)" null="YES" default=""/>
        <field name="Cod_Numerazione" type="smallint(6)" null="NO" default="1"
               comment="Codice numerazione per numerazioni comuni a piÃ¹ documenti e modelli"/>
        <field name="Json_Parametri" type="text" null="YES" />

        <index name="PRIMARY">
            <field name="Cod_Modello" seq="1"/>
        </index>

    </table>

    <table name="COG_Modelli_Righe" config="true" group="opengerp/accounting">
        <field name="Cod_Modello" type="varchar(50)" null="NO"  primary_key="yes"/>
        <field name="ID_Riga_Modello" type="smallint(6)" null="NO"  primary_key="yes"/>
        <field name="Cod_Causale_Contabile" type="varchar(50)" null="YES" default="" comment="da eliminare"/>

        <field name="Natura_Conto" type="varchar(20)" null="NO" default=""/>
        <field name="ID_Cliente" type="int(11)" null="YES" default=""/>
        <field name="Cod_Conto" type="varchar(50)" null="NO" default=""/>
        <field name="Tipo_Valore" type="varchar(20)" null="NO" default=""/>
        <field name="Segno_Riga_Modello" type="varchar(20)" null="NO" default=""/>

        <index name="PRIMARY">
            <field name="Cod_Modello" seq="1"/>
            <field name="ID_Riga_Modello" seq="2"/>
        </index>

    </table>

    <table name="DOC_Mezzitrasporto" config="true" group="opengerp/accouting">
        <field name="Cod_Mezzotrasporto" type="smallint(6)" null="NO" default="0" primary_key="yes"/>
        <field name="Des_Mezzotrasporto" type="varchar(50)" null="YES" default=""/>
        <field name="Cod_Mezzotrasporto_D" type="smallint(6)" null="NO" default="0"/>

        <index name="PRIMARY">
            <field name="Cod_Mezzotrasporto" seq="1"/>
        </index>

    </table>

    <table name="COG_Registri" config="true" dbobject="VatRegisters" group="opengerp/accounting">
        <field name="Cod_Registro" type="varchar(50)" null="NO"  primary_key="yes"/>
        <field name="Des_Registro" type="varchar(255)" null="NO" />
        <field name="Tipo_Registro" type="varchar(50)" null="NO" />
        <field name="Registro_UE" type="smallint(11)" null="NO" default="0"/>
        <field name="Sezionale" type="int(11)" null="NO" default="1"/>
        <field name="Utente" type="int(11)" null="NO" />

        <index name="PRIMARY">
            <field name="Cod_Registro" seq="1"/>
        </index>

    </table>

    <table name="DOC_Valute" config="true" group="opengerp/accounting">
        <field name="Cod_Valuta" type="char(3)" null="NO"  primary_key="yes"/>
        <field name="Des_Valuta" type="varchar(50)" null="NO" />
        <field name="Cod_Nazione" type="char(3)" null="NO" />
        <field name="Simbolo_Valuta" type="varchar(10)" null="NO" />
        <field name="Num_Decimali" type="smallint(6)" null="NO" default="2"/>
        <field name="Val_Cambio" type="decimal(20,6)" null="NO" default="1"/>
        <field name="Data_Cambio" type="date" null="YES" default="NULL" />

        <index name="PRIMARY">
            <field name="Cod_Valuta" seq="1"/>
        </index>

    </table>



    <table name="DOC_Pagamenti" config="true" dbobject="PaymentTerms" group="opengerp/accouting">
        <field name="Cod_Pagamento" type="varchar(50)" null="NO" default="" primary_key="yes"/>
        <field name="Cod_Tipo_Pagamento" type="varchar(10)" null="YES" default=""/>
        <field name="Perc_Sconto_Pagamento" type="double" null="NO" default="0"/>
        <field name="Des_Pagamento" type="varchar(255)" null="NO" default=""/>
        <field name="Num_Giorni" type="int(11)" null="YES" default=""/>
        <field name="Ab_Controllo_Saldo" type="smallint(6)" null="YES" default=""
               comment="se uguale 1 pagamento anticipato"/>
        <field name="Giorno_Fisso" type="int(11)" null="YES" default=""/>
        <field name="Val_Spese_Pag" type="decimal(20,6)" null="YES" default=""/>
        <field name="Ab_Fine_Mese" type="smallint(1)" null="YES" default=""/>
        <field name="Ab_Chiusura_Automatica" type="smallint(1)" null="YES" default="" comment="1 per incasso automatico alla conferma documento"/>
        <field name="Str_Slittamento" type="varchar(255)" null="YES" default=""/>
        <field name="Default_Pagamento" type="smallint(6)" null="NO" default="0"/>
        <field name="Str_Parametri" type="text" null="YES" default=""/>
        <field name="Data_Obsoleto" type="date" null="YES" default=""/>

        <index name="PRIMARY">
            <field name="Cod_Pagamento" seq="1"/>
        </index>

    </table>


    <table name="DOC_Pagamenti_Righe" config="true" dbobject="DueDates" group="opengerp/accouting">
        <field name="Cod_Pagamento" type="varchar(50)" null="NO" default="" primary_key="yes"/>
        <field name="ID_Pagamento_Riga" type="int(11)" null="NO" default="0" primary_key="yes"/>
        <field name="Giorni_Rata" type="int(11)" null="NO" default="0"/>
        <field name="Perc_Rata" type="double" null="NO" default="0"/>
        <field name="Cod_Tipo_Pagamento_Riga" type="varchar(10)" null="YES" default=""/>

        <index name="PRIMARY">
            <field name="Cod_Pagamento" seq="1"/>
            <field name="ID_Pagamento_Riga" seq="2"/>
        </index>

    </table>


    <table name="INT_Nazioni" config="true" dbobject="Countries" group="opengerp/core">
        <field name="Cod_Nazione" type="char(3)" null="NO" default="" primary_key="yes"/>
        <field name="Des_Nazione" type="varchar(30)" null="NO" default=""/>
        <field name="Des_Nazione_EN" type="varchar(50)" null="YES" default=""/>
        <field name="Cod_Iso" type="char(2)" null="YES" default=""/>
        <field name="Cod_Iso3" type="char(3)" null="NO" default=""/>
        <field name="Cod_Valuta" type="char(3)" null="YES" default=""/>
        <field name="Cod_Lingua" type="char(3)" null="YES" default=""/>
        <field name="P_Iva" type="int(11)" null="YES" default=""/>
        <field name="Cod_Iva_Default" type="varchar(20)" null="YES" default=""/>
        <field name="Cod_Cittadinanza" type="char(3)" null="YES" default=""/>
        <field name="Des_Cittadinanza" type="varchar(50)" null="YES" default=""/>
        <field name="Des_Nazione_Breve" type="varchar(50)" null="YES" default=""/>
        <field name="Cee" type="smallint(11)" null="NO" default="0"/>
        <field name="Blacklist" type="smallint(11)" null="NO" default="0"/>
        <field name="Cod_Fisco_Italia" type="char(3)" null="YES" default=""/>


        <index name="PRIMARY">
            <field name="Cod_Nazione" seq="1"/>
        </index>

    </table>

    <table name="COG_Esercizi" dbobject="AccountsFiscalYear" group="opengerp/accounting">
        <field name="Num_Esercizio" type="smallint(6)" null="NO"  primary_key="yes"/>
        <field name="Data_Inizio_Esercizio" type="date" null="NO" />
        <field name="Data_Fine_Esercizio" type="date" null="YES" default=""/>
        <field name="Stato_Esercizio" type="smallint(6)" null="NO" default="1"/>
        <field name="Json_Esercizio" type="text" null="YES" default=""/>
        <field name="Utente" type="int(11)" null="NO" />

        <index name="PRIMARY">
            <field name="Num_Esercizio" seq="1"/>
        </index>

    </table>


    <table name="DOC_Iva" config="true" dbobject="TaxCodes" group="opengerp/accounting">
        <field name="Cod_Iva" type="varchar(20)" null="NO" default="" primary_key="yes"/>
        <field name="Des_Iva" type="varchar(255)" null="YES" default=""/>
        <field name="Perc_Iva" type="decimal(7,4)" null="YES" default=""/>
        <field name="Perc_Indetraibile" type="decimal(7,4)" null="NO" default="0"/>
        <field name="CEE" type="smallint(6)" null="NO" default="0"/>
        <field name="Ab_Iva_Default" type="smallint(6)" null="NO" default="0"/>
        <field name="Ab_Iva_Default_Passiva" type="smallint(6)" null="YES" default=""/>
        <field name="Json_Dati_Iva" type="text" null="YES" default="0"
               comment="vett_parametri[natura] vedi config_dati_iva"/>
        <field name="Data_Obsoleto" type="date" null="YES" default='NULL'/>

        <index name="PRIMARY">
            <field name="Cod_Iva" seq="1"/>
        </index>

    </table>




</schema>

