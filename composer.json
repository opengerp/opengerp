{
  "name": "opengerp/opengerp",
  "description": "opengerp core open source ERP system web based ",
  "authors": [
    {
      "name": "Cognita Srl",
      "email": "info@gerp.it"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "files": ["src/legacy.php"],
    "psr-4": {
      "Opengerp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Opengerp\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-fileinfo": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-ftp": "*",
    "ext-odbc": "*",
    "ext-iconv": "*",
    "ext-sockets": "*",
    "symfony/expression-language": "5.4.*",
    "symfony/var-exporter": "^5.4",
    "symfony/event-dispatcher": "^5.4",
    "symfony/cache": "^5.4",
    "doctrine/lexer": "^2.1",
    "doctrine/instantiator": "^1.5",
    "smarty/smarty": "^3.1|^4.5",
    "studio-42/elfinder": "2.1.*",
    "opengerp/tcpdf": "dev-main",
    "ckeditor/ckeditor": "4.11.*",
    "eluceo/ical": "0.16.1",
    "dragonmantank/cron-expression": "*",
    "guzzlehttp/guzzle": "^7.3",
    "setasign/fpdi": "*",
    "setasign/fpdf": "*",
    "select2/select2": "^4.0",
    "michelf/php-markdown": "^2.0",
    "php-debugbar/php-debugbar": "*",
    "slim/slim": "4.*",
    "slim/psr7": "*",
    "zircote/swagger-php": "5.*",
    "opengerp/opengerp": "dev-main"

  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "^5.4",
    "psr/log": "^1"
  },
  "license": "MIT"
}
